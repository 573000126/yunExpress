                                                  <td valign="middle" align="center">
                                                      <div class="bar_area">
                                                          <div class="tc">
                                                              <img alt="" src="@Url.Action("BarCodeFile", "Print")?code=@Model.MailNo" />
                                                          </div>
                                                          <div class="F_Barcode">
                                                              @Model.MailNo
                                                          </div>
                                                      </div>
                                                  </td>
                                              </tr>
                                       </tbody></table>
                               </td>
                               <td>
                                   <table cellpadding="0" class="Countrycode_Tb">
                                       <tbody><tr>
                                                  <td align="center">
                                                      <img src="/images/SF-logo.jpg" width="70" /> <br>
                                                                                                   <span class="Countrycode_n">
                                                                                                       US
                                                                                                   </span>
                                                  </td>
                                              </tr>
                                       </tbody></table>
                               </td>
                           </tr>
                    </tbody></table>
            </div>
